{
    "compilerOptions": {
        "composite": true,
        "declaration": true,
        "declarationMap": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "inlineSources": false,
        "preserveWatchOutput": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "strict": true,
        "jsx": "react-jsx",
        "target": "ES2016",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "noEmit": true
    },
    "exclude": ["node_modules"]
}
