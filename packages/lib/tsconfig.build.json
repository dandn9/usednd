{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "sourceMap": true,
        "outDir": "./dist",
        "declarationMap": false,
        "declaration": true,
        "esModuleInterop": true,
        "composite": false,
        "noEmit": false,
        "importHelpers": true,
        "moduleResolution": "node",
        "incremental": false,
        "jsx": "react-jsx"
    },
    "exclude": ["./__tests__/**"]
}
